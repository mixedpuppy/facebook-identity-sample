{
  "name": "facebook chrome.identity sample",
  "version": "1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true
  },
  "permissions": [
    "identity",
    "https://www.facebook.com/",
    "https://graph.facebook.com/"
  ]
}